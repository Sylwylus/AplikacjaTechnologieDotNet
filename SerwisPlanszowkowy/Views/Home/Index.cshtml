@using System.Collections
@model IEnumerable<SerwisPlanszowkowy.ViewModels.NewsViewModel>
@{
    ViewBag.Title = "Home Page";
}


<div style="float:left; width:70%; ">
    <div style="align-content: center">
        <img style=" margin-left: 50px;" src="data:image/jpg;base64,">


    </div>
    <div style="padding-right: 20px;">
        <br />
        <hr />
        @if (User.IsInRole("Moderator"))
        {
            @Html.ActionLink("add news", "Create", "News")

        }

        @{
            int k = 0;
            String kreseczka = " | ";
            foreach (var item in Model)
            {
                k++;
                <h4><b>@Html.DisplayFor(modelItem => item.Title)</b></h4>
                <h6><i>@Html.DisplayFor(modelItem => item.PublishedDate), @Html.DisplayFor(modelItem => item.User.UserName)</i></h6>
                <p style="text-align: justify; padding-right: 40px;">
                    @Html.DisplayFor(modelItem => item.Content)
                </p>
                <br />
                if (User.IsInRole("Moderator"))
                {
                    @Html.ActionLink("edit", "Edit", "News", new { id = item.Id }, null) @kreseczka @Html.ActionLink("delete", "Delete", "News", new { id = item.Id }, null)
                }
                <br />
                <br />
                if (k == 10)
                {
                    <br /><br />
                    @Html.ActionLink("More News", "OldNews")
                    break;
                }
            }
        }
    </div>
</div>
<div style="float:right; width:30%; padding-left: 10px; ">
    <div>
        <h3>Top 5 Games</h3>
        <hr />
        @{ int u = 0; }
        <table>
            @foreach (var item in @ViewBag.game)
            {

                u++;
                <tr>
                    <td>
                        <b>  @u . </b>
                        <img src='@item.PhotoSource' style="max-height:75px; max-width:75px;" />
                        @Html.ActionLink((string)item.Name, "/Details/" + (int)item.Id, "Game")
                    </td>
                    <td style="padding-left: 20%;">@item.AvarageRate</td>
                </tr>
            }
        </table>
    </div>
    <div>
        <hr />
        <h3>Last added reviews</h3>
        <hr />
        @{

            foreach (var item in @ViewBag.review)
            {
                @Html.ActionLink((string)item.Title, "/Details/" + (int)item.Id, "Review")
                <br />
                @item.Game.Name
                <br />
                <i>@item.PublishedDate @item.User.UserName</i>
                <br /><br />

            }
        }
    </div>
</div>
<div style="clear:both"></div>